        <div class="display-field">
            @Html.DisplayFor(model => model.OrganizationSpectrumID)
        </div>
        <div class="label">
            @Html.DisplayNameFor(model => model.OrganizationWebsiteURL)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.OrganizationWebsiteURL)
        </div>
        <div class="label">
            @Html.DisplayNameFor(model => model.CreatedOn)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.CreatedOn)
